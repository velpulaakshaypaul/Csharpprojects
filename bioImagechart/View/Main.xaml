<Window x:Class="bioImagechart.View.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:View="clr-namespace:bioImagechart.View"
        Title="Main" MinWidth="300"
         SizeToContent="WidthAndHeight">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <View:ImageView  Grid.Row="0" Grid.Column="0" Margin="31,36,0,0"/>
        <View:ChartView Grid.Row="1" Grid.Column="1"/>
        <View:FFTView Grid.Row="0" Grid.Column="1"/>
        <View:PhaseView Grid.Row="1" Grid.Column="0"/>
    </Grid>
</Window>
